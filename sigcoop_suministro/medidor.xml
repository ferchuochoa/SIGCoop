<?xml version="1.0" encoding="UTF-8"?>
<tryton>
    <data>
        <!-- Forms y trees --> 
        <record model="ir.ui.view" id="medidor_view_form">
            <field name="model">sigcoop_suministro.medidor</field>
            <field name="type">form</field>
            <field name="name">medidor_form</field>
        </record>
        <record model="ir.ui.view" id="medidor_view_tree">
            <field name="model">sigcoop_suministro.medidor</field>
            <field name="type">tree</field>
            <field name="name">medidor_tree</field>
        </record>

        <!-- act_window --> 
        <record id="act_medidor_form" model="ir.action.act_window">
            <field name="name">medidor</field>
            <field name="res_model">sigcoop_suministro.medidor</field>
            <field name="search_value"/>
        </record>

        <!-- Menus --> 
        <menuitem parent="sigcoop_usuario.menu_usuario" sequence="0" action="act_medidor_form" icon="tryton-list" id="menu_medidor_form"/>
    </data>
</tryton>
