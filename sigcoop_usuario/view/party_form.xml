<?xml version="1.0"?>
<data>
    <xpath
        expr="/form/notebook/page[@id='accounting']"
        position="after">
        <page string="Datos cliente" id="datos_cliente">
            <label name="asociado"/>
            <field name="asociado"/>
            <label name="numero"/>
            <field name="numero"/>
            <label name="dir_entrega_factura"/>
            <field name="dir_entrega_factura"/>
            <label name="ruta"/>
            <field name="ruta"/>
            <label name="tipo_identificacion"/>
            <field name="tipo_identificacion"/>
            <label name="valor_identificacion"/>
            <field name="valor_identificacion"/>
            <label name="iva_condition"/>
            <field name="iva_condition"/>
            <label name="iibb_type"/>
            <field name="iibb_type"/>
            <label name="iibb_number"/>
            <field name="iibb_number"/>
        </page>
        <page string="Suministros" id="suministros">
            <field name="suministros" mode="tree,form" colspan="4"
                view_ids="sigcoop_usuario.suministro_view_tree,sigcoop_usuario.suministro_view_form"/>
        </page>
        <page string="Datos asociado" id="datos_asociado" states="{'invisible': Not(Bool(Eval('asociado')))}">
            <label name="numero_titulo"/>
            <field name="numero_titulo"/>
            <label name="fecha_ingreso"/>
            <field name="fecha_ingreso"/>
        </page>
        <page string="Rangos" id="rangos" states="{'invisible': Not(Bool(Eval('asociado')))}">
            <field name="rangos" mode="tree,form" colspan="4"
                view_ids="sigcoop_usuario.rango_view_tree,sigcoop_usuario.rango_view_form"/>
        </page>
        <page string="Familiares" id="familiares" states="{'invisible': Not(Bool(Eval('asociado')))}">
            <field name="familiares" mode="tree,form" colspan="4"
                view_ids="sigcoop_usuario.familiar_view_tree,sigcoop_usuario.familiar_view_form"/>
        </page>
        <page string="Aportes" id="aportes" states="{'invisible': Not(Bool(Eval('asociado')))}">
            <field name="aportes" mode="tree,form" colspan="4"
                view_ids="sigcoop_usuario.aporte_view_tree,sigcoop_usuario.aporte_view_form"/>
        </page>
    </xpath>
</data>
