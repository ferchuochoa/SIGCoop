<?xml version="1.0" encoding="UTF-8"?>
<tryton>
    <data>
        <!-- Forms y trees --> 
        <record model="ir.ui.view" id="suministro_view_form">
            <field name="model">sigcoop_usuario.suministro</field>
            <field name="type">form</field>
            <field name="name">suministro_form</field>
        </record>
        <record model="ir.ui.view" id="suministro_view_tree">
            <field name="model">sigcoop_usuario.suministro</field>
            <field name="type">tree</field>
            <field name="name">suministro_tree</field>
        </record>

        <!-- act_window --> 
        <record id="act_suministro_form" model="ir.action.act_window">
            <field name="name">Suministro</field>
            <field name="res_model">sigcoop_usuario.suministro</field>
            <field name="search_value"/>
        </record>

        <!-- Menus --> 
        <menuitem parent="menu_usuario" sequence="0" action="act_suministro_form" icon="tryton-list" id="menu_suministro_form"/>
    </data>
</tryton>
