<?xml version="1.0" encoding="UTF-8"?>
<tryton>
    <data>
        <!-- Forms y trees --> 
        <record model="ir.ui.view" id="familiar_view_form">
            <field name="model">sigcoop_usuario.familiar</field>
            <field name="type">form</field>
            <field name="name">familiar_form</field>
        </record>
        <record model="ir.ui.view" id="familiar_view_tree">
            <field name="model">sigcoop_usuario.familiar</field>
            <field name="type">tree</field>
            <field name="name">familiar_tree</field>
        </record>

        <!-- act_window --> 
        <record id="act_familiar_form" model="ir.action.act_window">
            <field name="name">Familiar</field>
            <field name="res_model">sigcoop_usuario.familiar</field>
            <field name="search_value"/>
        </record>

        <!-- Menus --> 
        <menuitem name="Familiar" sequence="0" id="menu_familiar"/>
        <menuitem parent="menu_familiar" sequence="0" action="act_familiar_form" icon="tryton-list" id="menu_familiar_form"/>
    </data>
</tryton>
