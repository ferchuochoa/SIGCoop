<?xml version="1.0" encoding="UTF-8"?>
<tryton>
    <data>
        <!-- Forms y trees --> 
        <record model="ir.ui.view" id="aporte_view_form">
            <field name="model">sigcoop_usuario.aporte</field>
            <field name="type">form</field>
            <field name="name">aporte_form</field>
        </record>
        <record model="ir.ui.view" id="aporte_view_tree">
            <field name="model">sigcoop_usuario.aporte</field>
            <field name="type">tree</field>
            <field name="name">aporte_tree</field>
        </record>

        <!-- act_window --> 
        <record id="act_aporte_form" model="ir.action.act_window">
            <field name="name">Aportes</field>
            <field name="res_model">sigcoop_usuario.aporte</field>
            <field name="search_value"/>
        </record>

        <!-- Menus --> 
        <menuitem name="Aporte" sequence="0" id="menu_aporte"/>
        <menuitem parent="menu_aporte" sequence="0" action="act_aporte_form" icon="tryton-list" id="menu_aporte_form"/>
    </data>
</tryton>
