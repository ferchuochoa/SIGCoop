        <record model="ir.ui.view" id="rango_view_form">
            <field name="model">sigcoop_usuario.rango</field>
            <field name="type" >form</field>
            <field name="name">rango_form</field>
        </record>
        <record model="ir.ui.view" id="rango_view_tree">
            <field name="model">sigcoop_usuario.rango</field>
            <field name="type" >tree</field>
            <field name="name">rango_tree</field>
        </record>
        <!-- act_window --> 
        <record id="act_rango_form" model="ir.action.act_window">
            <field name="name">Rango</field>
            <field name="res_model">sigcoop_usuario.rango</field>
            <field name="search_value"/>
        </record>

        <!-- Menus --> 
        <menuitem name="Rango" sequence="0" id="menu_rango"/>
        <menuitem parent="menu_rango" sequence="0" action="act_rango_form" icon="tryton-list" id="menu_rango_form"/>
    </data>
</tryton>
